{
  "name": "react-babel-bootstrap-webpack-starter",
  "version": "1.0.0",
  "description": "react + babel + bootstrap + webpack starter",
  "main": "src/index.js",
  "repository": {
    "private": true
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js --mode development",
    "dev": "webpack --config webpack.dev.config.js --mode development",
    "prod": "webpack --config webpack.prod.config.js --mode production",
    "serve": "webpack-dev-server -d --open",
    "build": "babel src/ -d lib/",
    "prepublish": "yarn run build",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:lint": "eslint \"**/*.{js,jsx}\" --quiet",
    "test:a11y": "pa11y http://localhost:8080/",
    "test:type": "flow"
  },
  "author": "Adrian Kinross",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flow-bin": "^0.75.0",
    "glob": "^7.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "nodemon": "^1.18.3",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.14.0",
    "purifycss-webpack": "^0.7.0",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "^16.4.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "4.8.3",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^8.1.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bootstrap": "^4.1.3",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.1",
    "cssnano": "^3.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.9.2",
    "pa11y": "^5.0.4",
    "popper.js": "^1.14.4",
    "postcss-flexbugs-fixes": "^4.1.0",
    "preact": "^8.3.0",
    "preact-compat": "^3.18.2",
    "purify-css": "^1.2.5",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.2",
    "react-lazy-load": "^3.0.13",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-motion": "^0.5.2",
    "react-overdrive": "^0.0.9",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.0",
    "sass-loader": "^7.1.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "uuid": "^3.3.2",
    "why-did-you-update": "^0.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/config/jest/setUpTestFramework.js",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/src/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/src/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test).js?(x)"
    ]
  }
}
